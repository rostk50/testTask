includes:
  - vendor/nunomaduro/larastan/extension.neon

parameters:
  level: max
  paths:
    - app
    - database
    - routes
  tmpDir: storage/framework/phpstan
  inferPrivatePropertyTypeFromConstructor: true
  reportUnmatchedIgnoredErrors: true
  excludePaths:
    analyse:
      - bootstrap/cache
      - storage
